<?xml version="1.0" encoding="UTF-8"?>
<job id="batchXmlConfigSample" xmlns="https://www.oracle.com/webfolder/technetwork/jsc/xml/ns/javaee/index.html" version="1.0">
	<step id="step1" next="step2">
		<chunk>
			<reader ref="reader"/>
			<processor ref="processor"/>
			<writer ref="writer"/>
		</chunk>
	</step>
	<step id="step2">
		<batchlet ref="batchlet">
			<properties>
				<property name="remoteServiceURL" value="#{jobParameters['remoteServiceURL']}"/>
			</properties>
		</batchlet>
	</step>
</job>
