<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns="https://www.springframework.org/schema/batch" xmlns:beans="https://www.springframework.org/schema/beans"
	xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://www.springframework.org/schema/batch https://www.springframework.org/schema/batch/spring-batch-2.1.xsd
	https://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<beans:import resource="common-context.xml" />
	
	<step id="step">
		<tasklet ref="tasklet"/>
	</step>

	<beans:bean id="tasklet" class="org.springframework.batch.core.configuration.xml.TestTasklet"/>

</beans:beans>